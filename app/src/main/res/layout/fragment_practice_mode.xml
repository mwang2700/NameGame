<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PracticeModeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableLayout
            android:id="@+id/tableLayout"
            android:layout_width="390dp"
            android:layout_height="390dp"
            android:layout_marginStart="18dp"
            android:layout_marginTop="275dp"
            android:layout_marginEnd="18dp"
            android:layout_marginBottom="275dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/practiceImage1"
                    android:layout_width="195dp"
                    android:layout_height="130dp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="27dp"
                    tools:srcCompat="@tools:sample/avatars" />

                <ImageView
                    android:id="@+id/practiceImage2"
                    android:layout_width="195dp"
                    android:layout_height="130dp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="27dp"
                    tools:srcCompat="@tools:sample/avatars" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/practiceImage3"
                    android:layout_width="195dp"
                    android:layout_height="130dp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="27dp"
                    tools:srcCompat="@tools:sample/avatars" />

                <ImageView
                    android:id="@+id/practiceImage4"
                    android:layout_width="195dp"
                    android:layout_height="130dp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="27dp"
                    tools:srcCompat="@tools:sample/avatars" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/practiceImage5"
                    android:layout_width="195dp"
                    android:layout_height="130dp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="27dp"
                    tools:srcCompat="@tools:sample/avatars" />

                <ImageView
                    android:id="@+id/practiceImage6"
                    android:layout_width="195dp"
                    android:layout_height="130dp"
                    tools:layout_editor_absoluteX="16dp"
                    tools:layout_editor_absoluteY="27dp"
                    tools:srcCompat="@tools:sample/avatars" />

            </TableRow>

        </TableLayout>

        <TextView
            android:id="@+id/practiceTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:layout_marginBottom="20dp"
            android:text="Person"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/tableLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.773" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>